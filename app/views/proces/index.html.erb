<% #breadcrumb :proce, @proce %>
<div class="container-fluid">
    <div class="row-fluid">
      <div class="span12">
        <div class="widget-box">
          <div class="widget-content nopadding">
            <table class="table table-bordered data-table">
              <thead>
                <tr>
                  <th>Sl</th>
                  <th>Division</th>
				  <th>Brand</th>
				  <th>Season</th>
				  <th>Year</th>
				  <th>Market</th>
				  <th>Customer Name</th>
				  <th>Customer Account</th>
				  <th>Project</th>
                  <th>Reference Style</th>
				  <th>Created Date</th>
                  <th colspan='3'>Actions</th>
                </tr>
              </thead>
              <tbody>
								<% @i=1 %>
								<% @proces.each do |proce| %>
									<tr class="gradeX">
										<td><%= @i %></td>
										<td><%= proce.division %></td>
								        <td><%= proce.brand %></td>
								        <td><%= proce.season %></td>
								        <td><%= proce.year %></td>
								        <td><%= proce.market %></td>
								        <td><%= proce.customername %></td>
								        <td><%= proce.customeraccount %></td>
								        <td><%= proce.project %></td>
								        <td><%= proce.referncestyle %></td>
										<td><%= proce.created_at %></td>
										<td><%= link_to 'Show', proce,class:'btn btn-success' %></td>
										<td><%= link_to 'Edit', edit_proce_path(proce),class:'btn btn-info' %></td>
										<td><%= link_to 'Destroy', proce, method: :delete, data: { confirm: 'Are you sure?' },class:'btn btn-danger' %></td>
									</tr>
									<% @i+=1%>
								<% end %>	
							</tbody>
            </table>
				
         	</div>
       </div>
   </div>
			<%= link_to 'New Process', new_proce_path ,class:'btn btn-success' %>
</div>
<!-- EOF -->
