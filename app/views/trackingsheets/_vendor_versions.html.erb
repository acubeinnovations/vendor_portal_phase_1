	<% data={'images'=>'Images','factory'=>'Factory','subcontractor'=>'Sub Contractor','salestrackingsheetedtosamplerequest'=>'Sales Proceed to Sample Request','customername'=>'Customer Name','customeraccount'=>'Customer Account','project'=>'Project','xmilldate'=>'X Mill Date','productionleadtime'=>'Production Lead Time', 'salesproceedtosamplerequest'=>'Sales Proceed to Sample Request','protoduebackfromvendor'=>'Proto Due Back From Vendor','daftovendordate'=>'Daf to Vendor Date','rtlmu'=>'RTL MU','mkup'=>'MK-UP','projectedunits'=>'Projected Units','targetfob'=>'Target FOB','targetmu'=>'Target MU','targetws'=>'Target WS','percolor'=>'Per Color','perstyle'=>'Per Style','incoterms'=>'Inco Terms','moq'=>'MOQ','thousandtofivethousandPCS'=>'1000 - 5000 PCS','fivethousandtotenthousandPCS'=>'5000 - 10000 PCS','ldpboat'=>'LDP Boat','ldpair'=>'LDP Air','startshipdate'=>'Start Ship Date','orderduedateviaboat'=>'Order Due Date Via Boat','orderduedateviaair'=>'Order Due Date Via Air','soss'=>'S/O OR S/S','nmbrofdefsmplneeded'=>'# of Dev Samples Needed','dafissued'=>'DAF Issued','sampleduedate'=>'Sample Due Date','daf2soss'=>'S/O OR S/S','daf2nmbrofdefsmplneeded'=>'# of Dev Samples Needed','daf2dafissued'=>'DAF Issued','daf2sampleduedate'=>'Sample Due Date','comments'=>'Comments'}%>
<div class="widget-box">
	<div class="widget-title font-size-10px">
		<span class="icon">
			<i class="icon-book"></i>
		</span>
		<h5>Previous Versions</h5>
	</div>
<div class="widget-content display-inline-block">
		<% i=0 %>
		<% exptarray=['version','user_id'] %>
	<div class="scroll-area" style="height:195px;" data-spy="scroll">
			<% @vendorversions.reverse.each  do |version| %>
			<% if !version.full_attributes['user_id'].blank? %>
				<% if User.find(version.full_attributes['user_id']).userrole=='vendor' && User.find(version.full_attributes['user_id']).email==User.find(params[:userid]).email %>
						<div class="widget-content nopadding">
													
													<% if Time.now.strftime("%Y-%m-%d") == version.updated_at.strftime("%Y-%m-%d")%> 									
				<div class="version_head">Today, <%= version.updated_at.strftime("%T %p")  %></div>
													
													<% else%>
													<div class="version_head"><%= version.updated_at.strftime("%d %b %Y, %T %p")  %></div>                                        <% end %> 
																<div class="trackingsheet-versions"> 
																			<div class="version_head"><i class="icon-stop"></i><% if !version.full_attributes['user_id'].blank? %><%= User.find(version.full_attributes['user_id']).email%><% end %></div>

													<ul id="toggle-view">
														<li>
															
															<span class="label label-success label-with-border-radius">Details<span class='plus-minus'>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;+</span></span><br>
															<div class="panel"> 
															 						
																		<% version.altered_attributes.each do |altattrbkey,altattrbval| %>
																		
										
																				<%	if !altattrbkey.in?(exptarray) %>

																					<div><% if !altattrbval.first.blank? || !altattrbval.second.blank? %> <%= data[altattrbkey] %> : <% if !altattrbval.first.blank? %> <%= altattrbval.first %> to <% end %><%= altattrbval.second %><% end %> </p></div>

																				<% end %>
																		<% end %>
									 								
								 									
								 							 	  
															</div>
														 </div>
														</li>
													</ul> 
												 </div>
												<% end %>
												<% end %>
												<% end %>



			</div>
		</div>
	</div>
</div>
			
