<%= simple_form_for @style, :url => styles_path, :html => { :multipart => true } do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :stylename %>
    <%= f.input :stylecode %>
    <%= f.input :users %>
    <%= f.input :image %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
