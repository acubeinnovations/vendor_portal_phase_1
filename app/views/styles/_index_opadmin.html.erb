<%  @styles.each do |style| %>
<%if !style.blank? %>
<tr class="gradeX">
<td> <%=@slno %></td>
<td><%=  style.try(:stylename) %></td>
<td><%= style.try(:stylecode) %></td>
<td><%= style.try(:protonumber) %></td>
<td><%= style.try(:division).try(:name) %></td>
<td><% @k=1 %><% if !style.designer_id.blank? %><%= @k.to_s+'. '+User.find(style.designer_id).email %><br><% @k+=1 %><% end %><% if !style.sales_id.blank? %><%= @k.to_s+'. '+User.find(style.sales_id).email %><br><% @k+=1 %><% end %><% if !style.operationuser_id.blank? %><%= @k.to_s+'. '+User.find(style.operationuser_id).email %><br><% @k+=1 %><% end %></td>
<td><%= image_tag "/vendorportal"+style.image.url(:small)  %></td>
<td><div style="text-align:center;"><%= link_to 'Edit',  edit_style_path(style) , :class => 'btn btn-warning btn-xs' %></div></td>
<td><div style="text-align:center;">
<%= link_to 'Delete', style, method: :delete, data: { header:"Style",confirm:(style.stylename)+' ?' },class:'btn btn-danger' %>	
</div> </td>
</tr>
<%end%>
<% @slno+=1%>
<% end %>	



