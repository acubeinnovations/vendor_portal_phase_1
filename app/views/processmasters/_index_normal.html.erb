
	<% @processmasters.each do |processmaster| %>
		<% exist=false%>
		<% processmaster.styles.each do |style| %>
		<% break if exist==true %>
		
		<%allowed_users=[style.designer_id.to_s,style.operationuser_id.to_s,style.sales_id.to_s]%>
		<% if current_user.id.to_s.in?(allowed_users) %>
		<% exist=true %>
		
		<tr class="gradeX">
			<td><input type="radio" name="selectprocess"></td>
			<td><%= @slno %></td>
			<td><% if !processmaster.division.blank? %><%= processmaster.division.name %><% end %></td>
			<td><%= processmaster.try(:name) %></td>
			<td><%= processmaster.try(:brand).try(:name) %></td>
			<td><%= processmaster.try(:season).try(:name) %></td>
			<td><%= processmaster.try(:market).try(:name) %></td>
			<td><%= processmaster.try(:submarket).try(:name) %></td>
			<td><%= processmaster.created_at %></td>
			<%  if current_user.userrole==VendorPortal::Application.config.designer  %>
				<td><%= processmaster.styles.where(:designer_id=>current_user.id).count %></td>
			<%  else if current_user.userrole==VendorPortal::Application.config.operationuser  %>
				<td><%= processmaster.styles.where(:operationuser_id=>current_user.id).count %></td>			
			<%  else if current_user.userrole==VendorPortal::Application.config.sales  %>
				<td><%= processmaster.styles.where(:sales_id=>current_user.id).count %></td>
			<% end %>	
			<% end %>
			<% end %>	
			<td>	<%= link_to 'Tracking Sheet', trackingsheets_path(:processmaster_id => processmaster.id),class:'btn btn-warning' %></td>
		</tr>	
	<% @slno+=1%>
	<% end %>	
<% end %>
<% end %>
