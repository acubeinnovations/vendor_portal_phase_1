<% @i=1 %>
	<% @processmasters.each do |processmaster| %>
			<% exist=false%>
			
			<% if current_user.division==processmaster.division %>
			
			<tr class="gradeX">
				<td><input type="radio" name="selectprocess"></td>
				<td><%= @i %></td>
				<td><% if !processmaster.try(:division).blank? %><%= processmaster.division.name %><% end %></td>
				<td><%= processmaster.try(:brand) %></td>
				<td><%= processmaster.try(:season) %></td>
				<td><%= processmaster.try(:market) %></td>
				<td><%= processmaster.created_at %></td>
				<td><%= link_to 'Edit', edit_processmaster_path(processmaster),class:'btn btn-info' %></td>
				<td><%= link_to 'Tracking Sheet', trackingsheets_path(:processmaster_id => processmaster.id),class:'btn btn-warning' %></td>
				
		</tr>	
	<% @i+=1%>

<% end %>
	<% end %>	
